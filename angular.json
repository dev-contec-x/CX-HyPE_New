{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "HyPE4PM": {
      "prefix": "hype",
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "assets": [
              {
                "glob": "**/*",
                "input": "./src/meta/",
                "output": "./meta/"
              },
              {
                "glob": "**/*",
                "input": "./src/assets/images/",
                "output": "./assets/images/"
              },
              {
                "glob": "**/*",
                "input": "./src/assets/fonts/",
                "output": "./assets/fonts/"
              },
              {
                "glob": "**/*",
                "input": "./src/assets/icons/",
                "output": "./assets/icons/"
              },
              "src/assets/favicon.ico",
              "src/manifest.webmanifest"
            ],
            "index": "src/index.html",
            "inlineStyleLanguage": "scss",
            "main": "src/main.ts",
            "namedChunks": true,
            "outputPath": "dist",
            "polyfills": "src/polyfills.ts",
            "scripts": [],
            "styles": [
              "src/assets/styles/base.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/assets/styles"
              ]
            },
            "tsConfig": "tsconfig.app.json",
            "vendorChunk": true,
            "serviceWorker": true,
            "ngswConfigPath": "ngsw-config.json"
          },
          "configurations": {
            "production": {
              "aot": true,
              "baseHref": "/",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "buildOptimizer": true,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "ngswConfigPath": "ngsw-config.json",
              "optimization": true,
              "outputHashing": "all",
              "serviceWorker": true,
              "sourceMap": false
            },
            "development": {
              "extractLicenses": false,
              "optimization": false,
              "sourceMap": true
            },
            "dev4Prod": {
              "baseHref": "/niku/custom/backlogBoard/"
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "development": {
              "browserTarget": "hype:build:development"
            },
            "production": {
              "browserTarget": "hype:build:production"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 3000
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "hype:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "configurations": {
            "development": {
              "karmaConfig": "src/karma.conf.js"
            },
            "production": {
              "karmaConfig": "src/karma-prod.conf.js"
            }
          },
          "options": {
            "assets": [
              "src/assets",
              "src/manifest.webmanifest",
              "src/manifest.webmanifest"
            ],
            "inlineStyleLanguage": "scss",
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "scripts": [],
            "styles": [
              "src/assets/styles/base.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles"
              ]
            },
            "tsConfig": "tsconfig.spec.json"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "schematicCollections": [
      "@angular-eslint/schematics"
    ],
    "analytics": "d6136ae8-d404-407c-8bff-754152b37b4d"
  }
}
