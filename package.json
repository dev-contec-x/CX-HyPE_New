{
  "description": "Contec-X HyPE4PM Application for projectmanagment",
  "name": "hype",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "analyze": "webpack-bundle-analyzer dist/stats.json",
    "build": "ng build",
    "build:dev": "ng build --configuration=development",
    "build:dev4prod": "ng build --configuration=dev4Prod",
    "build:prod": "ng build --configuration=production",
    "build:stats": "npm run build --stats-json",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "lint": "eslint -c .eslintrc.js --ext .ts ./src/app",
    "ng": "ng",
    "prepare": "husky install",
    "start": "ng serve --open",
    "test": "ng test",
    "test:dev": "ng test --configuration=development",
    "test:edge": "npm run test:dev --browsers Edge",
    "test:ff": "npm run test:dev --browsers Firefox",
    "test:headless": "npm run test:dev --watch=false --browsers=ChromeHeadless",
    "test:ie": "npm run test:dev --browsers IE",
    "test:prod": "ng test --configuration=production",
    "e2e": "ng e2e --devServerTarget=",
    "typedoc": "typedoc",
    "vhost": "ng serve --host 0.0.0.0"
  },
  "dependencies": {
    "@angular/animations": "^15.1.4",
    "@angular/cdk": "^15.1.4",
    "@angular/common": "^15.1.4",
    "@angular/compiler": "^15.1.4",
    "@angular/core": "^15.1.4",
    "@angular/forms": "^15.1.4",
    "@angular/material": "^15.1.4",
    "@angular/material-moment-adapter": "^15.1.4",
    "@angular/platform-browser": "^15.1.4",
    "@angular/platform-browser-dynamic": "^15.1.4",
    "@angular/pwa": "^15.1.5",
    "@angular/router": "^15.1.4",
    "@angular/service-worker": "^15.1.4",
    "@compodoc/compodoc": "^1.1.19",
    "@ngrx/store": "^15.3.0",
    "@types/quill": "^2.0.9",
    "angular-resizable-element": "^5.0.0",
    "angular2-hotkeys": "^13.1.0",
    "autolinker": "^3.15.0",
    "chart.js": "^3.9.0",
    "chartjs-plugin-annotation": "^2.0.0",
    "chartjs-plugin-datalabels": "^2.1.0",
    "fingerprintjs2": "^2.1.4",
    "linqts": "^1.14.4",
    "lit-html": "^2.2.7",
    "moment": "^2.29.4",
    "ng2-charts": "^4.0.0",
    "ngx-build-modern": "^1.1.10",
    "ngx-clipboard": "^15.1.0",
    "ngx-quill": "^18.0.0",
    "quill": "^1.3.7",
    "rxjs": "~7.5.6",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.5",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "^15.2.1",
    "@angular-eslint/eslint-plugin-template": "^15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.1.5",
    "@angular/compiler-cli": "^15.1.4",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.20.50",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.12.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.9.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "^7.0.4",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "stylelint": "^14.5.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-standard": "^25.0.0",
    "typedoc": "^0.23.25",
    "typescript": "~4.9.5",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "lint-staged": {
    "*.{ts, html}": "eslint --cache --fix",
    "*": "prettier --write --ignore-unknown"
  }
}
